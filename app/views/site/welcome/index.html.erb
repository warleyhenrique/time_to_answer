<h1>Site::Welcome#index</h1>
<p>Find me in app/views/site/welcome/index.html.erb</p>


<%= link_to 'Portal admin', admins_backoffice_welcome_index_path %>
<%= link_to 'Portal user', users_backoffice_welcome_index_path %>

<% if user_signed_in? %>
  <p>Usúario logado : <%= current_user.email %></p>
  <%= link_to 'Logoff-User', destroy_user_session_path, method: :delete %>
<% else %>
  <p>Usúario não logado</p>
<% end %>

<% if admin_signed_in? %>
  <p>Admin logado : <%= current_admin.email %></p>
  <%= link_to 'Logoff-Admin', destroy_admin_session_path, method: :delete %>
<% else %>
  <p>Administrador não logado</p>
<% end %>